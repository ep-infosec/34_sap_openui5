<mvc:View
	controllerName="sap.f.cardsdemo.controller.CsrfToken"
	xmlns:w="sap.ui.integration.widgets"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<f:GridContainer>
		<w:Card id="card1" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/csrfTokens/cardManifest.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData columns="3" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card2" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/csrfTokens/cardManifestTokenPath.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData columns="3" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card3" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/csrfTokens/cardManifestExpiringToken.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData columns="3" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card4" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/csrfTokens/cardManifestHost.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData columns="3" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card5" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/csrfTokens/cardManifestHostInvalidTokenURL.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData columns="3" />
			</w:layoutData>
		</w:Card>
		<w:Card id="card6" manifest="{cardsPlayground>/playgroundBaseUrl}/cardcontent/csrfTokens/cardManifestParams.json">
			<w:layoutData>
				<f:GridContainerItemLayoutData columns="3" />
			</w:layoutData>
		</w:Card>
	</f:GridContainer>
</mvc:View>